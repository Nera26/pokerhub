{
  "dashboard": {
    "title": "Room Worker Resources",
    "panels": [
      {
        "type": "timeseries",
        "title": "CPU Usage",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"room-worker\"}[1m])"
          }
        ]
      },
      {
        "type": "timeseries",
        "title": "Memory Usage",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"room-worker\"}"
          }
        ]
      }
    ]
  }
}
