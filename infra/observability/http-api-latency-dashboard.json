{
  "dashboard": {
    "title": "HTTP API Latency",
    "labels": {
      "pagerduty_service": "pokerhub-sre",
      "runbook": "docs/runbooks/http-api-latency.md"
    },
    "panels": [
      {
        "type": "timeseries",
        "title": "p99 latency",
        "datasource": "Prometheus",
        "targets": [
          { "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))" }
        ]
      }
    ]
  }
}
