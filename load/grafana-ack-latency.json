{
  "title": "PokerHub ACK Latency",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "ACK latency p50/p95/p99",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(ack_latency_bucket[1m])) by (le))",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(ack_latency_bucket[1m])) by (le))",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(ack_latency_bucket[1m])) by (le))",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Global action count",
      "targets": [
        {
          "expr": "max_over_time(game_action_global_count_sum[1m])",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ]
    }
  ]
}
