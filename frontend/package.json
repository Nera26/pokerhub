{
  "name": "poker-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_INTL_CONFIG=next-intl.config.ts next dev",
    "prebuild": "npm run generate:api-types --prefix ..",
    "build": "NEXT_INTL_CONFIG=next-intl.config.ts next build",
    "start": "NEXT_INTL_CONFIG=next-intl.config.ts next start",
    "start:test": "NEXT_INTL_CONFIG=next-intl.config.ts next start -p 3000",
    "lint": "eslint .",
    "pretypecheck": "npm run generate:api-types --prefix ..",
    "typecheck": "tsc --noEmit",
    "pretest:e2e": "playwright install --with-deps",
    "pretest:a11y": "playwright install --with-deps",
    "test": "NEXT_PUBLIC_E2E=0 jest --coverage",
    "test:e2e": "http_proxy= https_proxy= HTTP_PROXY= HTTPS_PROXY= NO_PROXY=localhost,127.0.0.1 NEXT_PUBLIC_E2E=1 playwright test --retries=2 --fail-on-flaky-tests",
    "test:a11y": "http_proxy= https_proxy= HTTP_PROXY= HTTPS_PROXY= NO_PROXY=localhost,127.0.0.1 NEXT_PUBLIC_E2E=1 playwright test e2e/accessibility.spec.ts --retries=2 --fail-on-flaky-tests",
    "test:e2e:integration": "http_proxy= https_proxy= HTTP_PROXY= HTTPS_PROXY= NO_PROXY=localhost,127.0.0.1 NODE_PATH=./node_modules NEXT_PUBLIC_E2E=0 playwright test e2e/gameplay.spec.ts e2e/lobby-tables.spec.ts --config=playwright.integration.config.ts --retries=0",
    "playwright:install": "playwright install --with-deps",
    "format": "prettier --write .",
    "prepare": "husky install",
    "test:perf": "node scripts/lighthouse.mjs",
    "bundle:check": "node scripts/check-bundles.mjs",
    "lighthouse:ci": "lhci autorun --config=.lighthouserc.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:visual": "chromatic --exit-zero-on-changes",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.10.0",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-virtual": "^3.13.9",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "jest-environment-jsdom": "^30.0.5",
    "minimatch": "^10.0.3",
    "next": "15.3.3",
    "next-intl": "^3.26.5",
    "next-pwa": "^5.6.0",
    "nextjs-toploader": "^3.8.16",
    "playwright": "^1.55.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.62.0",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@chromatic-com/storybook": "^4.1.1",
    "@ducanh2912/next-pwa": "^10.2.9",
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^15.3.3",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/nextjs-vite": "^9.1.3",
    "@storybook/testing-library": "^0.2.0",
    "@tailwindcss/postcss": "^4.1.10",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.12",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^20.19.11",
    "@types/nprogress": "^0.2.3",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "chromatic": "^13.1.3",
    "chrome-launcher": "^0.15.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-storybook": "^9.1.3",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jest-openapi": "^0.14.2",
    "lighthouse": "^12.8.1",
    "lint-staged": "^15.5.2",
    "msw": "^2.10.5",
    "postcss": "^8.5.5",
    "prettier": "^3.6.2",
    "storybook": "^9.1.3",
    "tailwindcss": "^4.1.10",
    "typescript": "^5",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": "prettier --write"
  }
}
