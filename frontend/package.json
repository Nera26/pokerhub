{
  "name": "poker-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "verify:contract": "tsx scripts/check-contract-version.ts",
    "prebuild": "npm run verify:contract && npm run contracts:generate --prefix ..",
    "build": "next build",
    "start": "next start",
    "start:test": "next start -p 3000",
    "lint": "eslint .",
    "pretypecheck": "npm run contracts:generate --prefix ..",
    "typecheck": "tsc --noEmit",
    "pretest": "npm run contracts:generate --prefix ..",
    "pretest:e2e": "cross-env PLAYWRIGHT_DOWNLOAD_HOST=https://playwright.azureedge.net playwright install --with-deps",
    "pretest:a11y": "cross-env PLAYWRIGHT_DOWNLOAD_HOST=https://playwright.azureedge.net playwright install --with-deps",
    "test": "cross-env NEXT_PUBLIC_E2E=0 jest --coverage",
    "test:contracts": "jest --testPathPatterns=.contract.spec.ts$",
    "test:e2e": "cross-env PLAYWRIGHT_DOWNLOAD_HOST=https://playwright.azureedge.net http_proxy= https_proxy= HTTP_PROXY= HTTPS_PROXY= NO_PROXY=localhost,127.0.0.1 NEXT_PUBLIC_E2E=1 playwright test --retries=2 --fail-on-flaky-tests --config=playwright.config.ts",
    "test:a11y": "cross-env PLAYWRIGHT_DOWNLOAD_HOST=https://playwright.azureedge.net http_proxy= https_proxy= HTTP_PROXY= HTTPS_PROXY= NO_PROXY=localhost,127.0.0.1 NEXT_PUBLIC_E2E=1 playwright test e2e/accessibility.spec.ts --retries=2 --fail-on-flaky-tests --config=playwright.config.ts",
    "playwright:install": "cross-env PLAYWRIGHT_DOWNLOAD_HOST=https://playwright.azureedge.net playwright install --with-deps",
    "format": "prettier --write .",
    "prepare": "cd .. && husky frontend/.husky",
    "test:perf": "node scripts/lighthouse.mjs",
    "bundle:check": "node scripts/check-bundles.mjs",
    "lighthouse:ci": "lhci autorun --config=.lighthouserc.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:visual": "chromatic --exit-zero-on-changes",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-brands-svg-icons": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "@hookform/resolvers": "^3.10.0",
    "@tanstack/react-query": "^5.90.1",
    "@tanstack/react-virtual": "^3.13.9",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "next": "15.5.2",
    "next-intl": "^3.26.5",
    "next-pwa": "^5.6.0",
    "nextjs-toploader": "^3.8.16",
    "playwright": "^1.55.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.62.0",
    "socket.io-client": "^4.8.1",
    "socket.io-parser": "^4.2.4",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0",
    "workbox-background-sync": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^15.5.2",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/nextjs-vite": "^9.1.3",
    "@storybook/react": "^9.1.3",
    "@tanstack/react-query-devtools": "^5.85.6",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.3.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.2.4",
    "@jest/globals": "^29.7.0",
    "axe-core": "^4.10.3",
    "node-fetch": "^2.6.1",
    "chromatic": "^13.1.4",
    "chrome-launcher": "^0.15.2",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "msw": "^2.4.9",
    "lighthouse": "^12.8.2",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "storybook": "^9.1.3",
    "tailwindcss": "^4.1.10",
    "tsx": "^4.19.1",
    "typescript": "^5",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "jest-environment-jsdom": "^30.1.2"
  },
  "overrides": {
    "axios": "1.11.0",
    "tmp": "0.2.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": "prettier --write"
  }
}
