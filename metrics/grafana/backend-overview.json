{
  "title": "Backend Overview",
  "schemaVersion": 30,
  "version": 1,
  "panels": [
    {
      "id": 1,
      "type": "graph",
      "title": "HTTP Requests",
      "datasource": "Cloud Monitoring",
      "targets": [
        {
          "query": "fetch custom.googleapis.com/backend/http_requests | align rate(1m) | group_by [metric.status], [value_http_requests: sum(value.http_requests)]"
        }
      ]
    },
    {
      "id": 2,
      "type": "graph",
      "title": "Log Records",
      "datasource": "Cloud Monitoring",
      "targets": [
        {
          "query": "fetch custom.googleapis.com/backend/log_records | align rate(1m) | group_by [metric.severity], [value_log_records: sum(value.log_records)]"
        }
      ]
    }
  ]
}
