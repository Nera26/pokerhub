{
  "dashboard": {
    "title": "Action ACK Latency",
    "panels": [
      {
        "type": "timeseries",
        "title": "Action ACK Latency",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, sum(rate(game_action_ack_latency_ms_bucket{event=\"action\"}[5m])) by (le))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(game_action_ack_latency_ms_bucket{event=\"action\"}[5m])) by (le))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(game_action_ack_latency_ms_bucket{event=\"action\"}[5m])) by (le))",
            "legendFormat": "p99"
          }
        ]
      },
      {
        "type": "timeseries",
        "title": "ACK Latency Burn Rate",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "action_ack_latency_error_budget_burn_rate_1h",
            "legendFormat": "1h"
          },
          {
            "expr": "action_ack_latency_error_budget_burn_rate_6h",
            "legendFormat": "6h"
          }
        ]
      }
    ]
  }
}
