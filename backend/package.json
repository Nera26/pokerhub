{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "bin": {
    "export-proofs": "src/scripts/export-proofs.ts"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "fixtures:icm": "ts-node --transpile-only test/fixtures/icm/generate.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/data-source.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/database/data-source.ts src/database/migrations/$npm_config_name",
    "migration:create": "typeorm-ts-node-commonjs migration:create src/database/migrations/$npm_config_name",
    "export:proofs": "ts-node src/scripts/export-proofs.ts",
    "rebuild:leaderboard": "ts-node src/leaderboard/rebuild.ts",
    "benchmark:leaderboard": "ts-node src/leaderboard/rebuild.ts --benchmark",
    "analytics:consume": "ts-node src/analytics/consume.ts",
    "load:socket": "ts-node test/load/socket-load.ts",
    "test:soak": "node --trace-gc -r ts-node/register test/soak/soak24h.ts",
    "seed:test-credentials": "ts-node src/scripts/seed-test-credentials.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.876.0",
    "@aws-sdk/s3-request-presigner": "^3.876.0",
    "@clickhouse/client": "^1.12.1",
    "@elastic/elasticsearch": "^8.19.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.0.1",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.6",
    "@opentelemetry/auto-instrumentations-node": "^0.50.2",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.203.0",
    "@opentelemetry/exporter-prometheus": "^0.50.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.50.0",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.50.0",
    "ajv": "^8.17.1",
    "amqplib": "^0.10.9",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.2",
    "cache-manager": "^7.2.0",
    "cache-manager-ioredis": "^2.1.0",
    "cookie-parser": "^1.4.7",
    "helmet": "^7.2.0",
    "ioredis": "^5.7.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "nestjs-pino": "^4.4.0",
    "onfido": "^4.3.0",
    "p-queue": "^6.6.2",
    "parquetjs-lite": "^0.8.1",
    "pg": "^8.16.3",
    "pino-http": "^8.6.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.26",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "fast-check": "^3.21.0",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "js-yaml": "^4.1.0",
    "pg-mem": "^3.0.5",
    "prettier": "^3.4.2",
    "socket.io-client": "^4.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "testcontainers": "^11.5.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "zod": "^3.25.76",
    "zod-fast-check": "^0.10.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test/.*\\.e2e-spec\\.ts$"
    ],
    "moduleNameMapper": {
      "^@shared/(.*)$": "<rootDir>/../shared/$1"
    },
    "modulePaths": [
      "<rootDir>/node_modules"
    ]
  }
}
